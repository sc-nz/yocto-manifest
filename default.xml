<?xml version="1.0" encoding="UTF-8"?>
<!--
This is a copy of the file at https://github.com/digi-embedded/dey-manifest/blob/rocko/default.xml
using sc-nz mirrors and the repository revisions replaced with the specific SHAs. If this build is
to be promoted to a stable build of biosphere then the contents of this file should be committed as
https://github.com/sc-nz/yocto-manifest/blob/master/default.xml. Thereafter a stable build of biosphere
will use the same repo revisions as this build did.
-->
<manifest>
  <default sync-j="4" revision="rocko"/>

  <remote name="digi"  fetch="ssh://git@github.com/sc-nz"/>
  <remote name="fsl"   fetch="ssh://git@github.com/sc-nz"/>
  <remote name="qt5"   fetch="ssh://git@github.com/sc-nz"/>
  <remote name="swu"   fetch="ssh://git@github.com/sc-nz"/>
  <remote name="oe"    fetch="ssh://git@github.com/sc-nz"/>
  <remote name="yocto" fetch="ssh://git@github.com/sc-nz"/>

  <project name="meta-digi.git" path="sources/meta-digi" remote="digi" revision="1246ecff2cecea9247d94f36385608ac844d7abb">
    <copyfile src="sdk/mkproject.sh" dest="mkproject.sh"/>
  </project>
  <project name="meta-freescale.git"    path="sources/meta-freescale"    remote="yocto" revision="d6141ea291a1ac9ab8fb1dd1110d408f840fda57"/>
  <project name="meta-fsl-demos.git"    path="sources/meta-fsl-demos"    remote="fsl"   revision="0ec6d7e206705702b5b534611754de0787f92b72"/>
  <project name="meta-openembedded.git" path="sources/meta-openembedded" remote="oe"    revision="dacfa2b1920e285531bec55cd2f08743390aaf57"/>
  <project name="meta-qt5.git"          path="sources/meta-qt5"          remote="qt5"   revision="7a52f924a0298857db99be20aca1484bb2b4e163"/>
  <project name="meta-swupdate.git"     path="sources/meta-swupdate"     remote="swu"   revision="ca89e377482015ff9807179fc224cc2f5f9c65a6"/>
  <project name="poky.git"              path="sources/poky"              remote="yocto" revision="fdeecc901196bbccd7c5b1ea4268a2cf56764a62"/>
  <project name="meta-virtualization.git" path="sources/meta-virtualization" remote="yocto" revision="bd77388f31929f38e7d4cc9c711f0f83f563007e"/>
</manifest>
